<div class='cards'>
  <% @groups.each do |group| %>
    <div class="card">
      <div class="card-body d-flex">
        <div class="image">
          <div> <%= group.icon %> </div>
        </div>
        <div>
          <p> <%= group.name %> </p>
          <p> <%= group. %> </p>
        </div>
      </div>
    </div>
  <%end%>
</div>